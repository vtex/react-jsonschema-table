{
  "name": "react-jsonschema-table",
  "version": "1.0.3",
  "description": "Boilerplate for publishing modern React modules with Rollup",
  "author": "Travis Fischer",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url":
      "git@github.com:transitive-bullshit/react-modern-library-boilerplate.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "scripts": {
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepublish": "npm run build",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "dependencies": {
    "ajv": "^5.2.2",
    "ajv-i18n": "^2.1.1",
    "es6-promise-pool": "^2.5.0",
    "intl": "^1.2.5",
    "json2csv": "^3.5.0",
    "moment": "^2.18.1",
    "react-bootstrap": "^0.31.0",
    "react-dropzone": "^3.13.4",
    "react-hotkeys": "^0.10.0",
    "react-intl": "^2.3.0",
    "react-slick": "^0.15.0",
    "react-widgets": "^4.0.0",
    "react-widgets-moment": "^4.0.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "reflux": "^6.4.1",
    "underscore": "^1.8.3",
    "vtex-tachyons": "^0.1.5"
  },
  "peerDependencies": {
    "react": "^15.x || ^16.x",
    "react-dom": "^15.x || ^16.x",
    "prop-types": "^15.5.4"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^21.2.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "enzyme": "^3.1.0",
    "eslint": "^4.9.0",
    "eslint-config-vtex": "^7.0.0",
    "eslint-config-vtex-react": "^3.0.3",
    "gh-pages": "^1.0.0",
    "jest": "^21.2.1",
    "react-test-renderer": "^16.0.0",
    "redux-mock-store": "^1.3.0",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-less": "^0.1.3",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-postcss": "^0.5.5"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "globals": {
      "NODE_ENV": "test"
    },
    "moduleFileExtensions": ["js", "jsx"]
  },
  "files": ["dist"]
}

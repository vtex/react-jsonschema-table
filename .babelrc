{
  "env": {
    "production": {
      "presets": [
        "react",
        [
          "es2015",
          {
            "modules": false
          }
        ]
      ],
      "plugins": [
        "external-helpers",
        "transform-class-properties",
        "transform-object-rest-spread",
        [
          "transform-runtime",
          {
            "polyfill": false,
            "regenerator": true
          }
        ]
      ]
    },
    "test": {
      "presets": [
        "react",
        [
          "es2015",
          {
            "modules": false
          }
        ]
      ],
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    },
    "development": {
      "presets": [
        "react",
        [
          "es2015",
          {
            "modules": false
          }
        ]
      ],
      "plugins": [
        "external-helpers",
        "transform-class-properties",
        "transform-object-rest-spread",
        [
          "transform-runtime",
          {
            "polyfill": false,
            "regenerator": true
          }
        ]
      ]
    }
  }
}